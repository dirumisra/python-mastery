    "})\n",
    "\n",
    "# Display original aggregated DataFrame (multi-level columns)\n",
    "print(\"Aggregated with Multi-level Columns:\\n\", category_agg)\n",
    "\n",
    "# ------------------------\n",
    "# Flatten multi-level columns for easier access\n",
    "# - Combine tuple column names into single strings\n",
    "category_agg.columns = ['_'.join(col).strip() for col in category_agg.columns.values]\n",
    "\n",
    "# Display flattened DataFrame\n",
    "print(\"\\nAggregated with Flattened Columns:\\n\", category_agg)"
   ]
